<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$>
<head>
  <meta charset="utf-8">
  <meta name="generator" content="pandoc">
$for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
  <title>$if(title-prefix)$$title-prefix$ - $endif$$pagetitle$</title>
  <style type="text/css">code{white-space: pre;}</style>
$if(quotes)$
  <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
$if(highlighting-css)$
  <style type="text/css">
$highlighting-css$
  </style>
$endif$
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$
$if(math)$
  $math$
$endif$
$for(header-includes)$
  $header-includes$
$endfor$
</head>
<body>
$for(include-before)$
$include-before$
$endfor$
$if(title)$
<header>
$if(date)$
<h3 class="date">$date$</h3>
$endif$

<h1 class="title">$title$</h1>

<h2 class="author">$for(author)$$author.name$<sup>$author.affilnum$</sup>$sep$, $endfor$</h2>

$for(affiliation)$
<h5 class="affilation"><sup>$affiliation.key$</sup>$affiliation.name$</h5>
$endfor$

</header>
$endif$

$if(abstract)$

<p class="small"><strong>Abstract: </strong><em>$abstract$</em></p>

$endif$


$body$
</body>
</html>
